<html>
    <head>
       <link rel="stylesheet" href="css/style.css">        
    </head>
        <body id="resBod">
            <h1 id="resTitle">Workout Overview</h1>
            <h3 id="rounds"><%= @workout.rounds %>:</h3>
            <ul id="routine">
                <img src="../images/ResultPic.jpg" id="resPic">
                <% @workout.wod %>
                <% @workout.wod.each do |rep| %>
                <li><%= rep + "<br>" %></li>
                <% end %>
            </ul>
            <form action="https://www.anytimefitness.com/find-gym/">
                <button id="gymLoc">
                    Find a gym near you!
                </button>
            </form>
        </body>
</html>